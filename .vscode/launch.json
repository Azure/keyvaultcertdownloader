    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch-Windows",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {
                "GOOS":"windows"
            },
            "args": [
                "--certurl","https://pmc-anf-release-kv.vault.azure.net/vm-cert2",
                "--outputfolder","c:\\temp",
                "--environment","AZUREUSGOVERNMENTCLOUD"
            ],
            "useApiV1": false,
            "dlvLoadConfig": {
                "followPointers": true,
                "maxVariableRecurse": 1,
                "maxStringLen": 500,
                "maxArrayValues": 500,
                "maxStructFields": -1
            }
        }
    ]
}